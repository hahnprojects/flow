{
  "name": "flow-module-examples",
  "version": "1.4.4",
  "description": "Flow Module Examples",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "flow build",
    "lint": "eslint '*/**/*.{js,ts}' --fix",
    "format": "flow format",
    "test": "flow test",
    "prepackage": "flow lint",
    "package": "flow package",
    "publish": "flow publish-module",
    "generate-schemas": "flow generate-schemas"
  },
  "dependencies": {
    "@hahnpro/flow-sdk": "workspace:*",
    "class-validator": "~0.14.0",
    "python-shell": "^3.0.1"
  },
  "devDependencies": {
    "@hahnpro/flow-cli": "workspace:*",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "commonjs": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "ignorePatterns": [
      "dist/"
    ],
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/no-var-requires": "off",
      "no-async-promise-executor": "off",
      "no-console": "error"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 140,
    "singleQuote": true,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.ts",
        "options": {
          "parser": "typescript"
        }
      }
    ]
  }
}
