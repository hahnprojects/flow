{
  "name": "@hahnpro/flow-sdk",
  "version": "9.6.2",
  "description": "SDK for building Flow Modules",
  "license": "MIT",
  "author": {
    "name": "Hahn Projects GmbH",
    "url": "https://hahnpro.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hahnprojects/flow.git"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.lib.json",
    "build:nocomments": "../../node_modules/.bin/tsc -p tsconfig.nocomments.json",
    "postbuild": "../../node_modules/.bin/copyfiles -u 1 lib/*.py dist",
    "prepublishOnly": "pnpm build && pnpm build:nocomments && pnpm postbuild",
    "lint": "eslint '*/**/*.{js,ts}'"
  },
  "dependencies": {
    "@hahnpro/hpc-api": "2025.3.7",
    "@nats-io/jetstream": "3.2.0",
    "@nats-io/nats-core": "3.2.0",
    "@nats-io/transport-node": "3.2.0",
    "amqp-connection-manager": "4.1.15",
    "amqplib": "0.10.9",
    "class-transformer": "0.5.1",
    "class-validator": "~0.14.2",
    "cloudevents": "10.0.0",
    "lodash": "4.17.21",
    "object-sizeof": "~2.6.5",
    "python-shell": "5.0.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "string-interp": "0.3.6"
  },
  "devDependencies": {
    "@types/amqplib": "0.10.7",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.20",
    "@types/node": "22.18.8",
    "class-validator-jsonschema": "5.1.0",
    "jest": "30.2.0",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "axios": "1.12.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "lodash": "4.17.21",
    "python-shell": "5.x"
  },
  "engines": {
    "node": ">=v22"
  }
}
