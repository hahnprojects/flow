{
  "name": "@hahnpro/flow-sdk",
  "version": "5.0.2",
  "description": "SDK for building Flow Modules",
  "license": "MIT",
  "author": {
    "name": "Hahn Projects GmbH",
    "url": "https://hahnpro.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hahnprojects/flow.git"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.lib.json",
    "build:nocomments": "../../node_modules/.bin/tsc -p tsconfig.nocomments.json",
    "postbuild": "../../node_modules/.bin/copyfiles -u 1 lib/*.py dist",
    "prepublishOnly": "pnpm build && pnpm build:nocomments && pnpm postbuild",
    "lint": "eslint '*/**/*.{js,ts}'"
  },
  "dependencies": {
    "@hahnpro/hpc-api": "workspace:*",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "class-transformer": "0.5.1",
    "class-validator": "~0.14.0",
    "cloudevents": "^8.0.0",
    "lodash": "^4.17.21",
    "object-sizeof": "~2.6.3",
    "python-shell": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "string-interp": "^0.3.6"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "class-validator-jsonschema": "^5.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "axios": "1.x",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "lodash": "4.x",
    "python-shell": "3.x"
  },
  "engines": {
    "node": ">=v18"
  }
}
