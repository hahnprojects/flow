{
  "name": "@hahnpro/flow-sdk",
  "version": "8.0.9",
  "description": "SDK for building Flow Modules",
  "license": "MIT",
  "author": {
    "name": "Hahn Projects GmbH",
    "url": "https://hahnpro.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hahnprojects/flow.git"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.lib.json",
    "build:nocomments": "../../node_modules/.bin/tsc -p tsconfig.nocomments.json",
    "postbuild": "../../node_modules/.bin/copyfiles -u 1 lib/*.py dist",
    "prepublishOnly": "pnpm build && pnpm build:nocomments && pnpm postbuild",
    "lint": "eslint '*/**/*.{js,ts}'"
  },
  "dependencies": {
    "@hahnpro/hpc-api": "2025.2.4",
    "@nats-io/nats-core": "3.0.0-54",
    "@nats-io/transport-node": "3.0.0-38",
    "amqp-connection-manager": "4.1.14",
    "amqplib": "0.10.5",
    "class-transformer": "0.5.1",
    "class-validator": "~0.14.1",
    "cloudevents": "8.0.2",
    "lodash": "4.17.21",
    "object-sizeof": "~2.6.5",
    "python-shell": "5.0.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "string-interp": "0.3.6"
  },
  "devDependencies": {
    "@types/amqplib": "0.10.7",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.16",
    "@types/node": "22.13.10",
    "class-validator-jsonschema": "5.0.2",
    "jest": "29.7.0",
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "axios": "1.8.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "lodash": "4.17.21",
    "python-shell": "5.x"
  },
  "engines": {
    "node": ">=v18"
  }
}
