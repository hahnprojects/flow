{
  "name": "@hahnpro/flow-sdk",
  "version": "4.19.1",
  "description": "SDK for building Flow Modules",
  "license": "MIT",
  "author": {
    "name": "Hahn Projects GmbH",
    "url": "https://hahnpro.com"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:hahnpro/flow.git"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.lib.json",
    "build:nocomments": "../../node_modules/.bin/tsc -p tsconfig.nocomments.json",
    "postbuild": "../../node_modules/.bin/copyfiles -u 1 lib/*.py dist",
    "prepublishOnly": "pnpm build && pnpm build:nocomments && pnpm postbuild"
  },
  "dependencies": {
    "@hahnpro/hpc-api": "workspace:*",
    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "class-transformer": "0.4.0",
    "class-validator": "~0.13.1",
    "class-validator-jsonschema": "^3.1.0",
    "cloudevents": "^5.0.0",
    "lodash": "^4.17.21",
    "object-sizeof": "^1.6.1",
    "python-shell": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "string-interp": "^0.3.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@golevelup/nestjs-rabbitmq": "^1.18.0",
    "@types/amqp-connection-manager": "^2.0.12",
    "@types/amqplib": "^0.8.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/node": "^14.17.22",
    "jest": "^27.3.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=v14.13"
  }
}
