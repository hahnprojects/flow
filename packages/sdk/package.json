    "class-validator": "~0.14.0",
    "cloudevents": "^7.0.0",
    "lodash": "^4.17.21",
    "object-sizeof": "~2.4.2",
    "python-shell": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "string-interp": "^0.3.6"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.6",
    "class-validator-jsonschema": "^5.0.0",
