{
  "name": "@hahnpro/flow-cli",
  "version": "2.17.17",
  "description": "CLI for managing Flow Modules",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Hahn Projects GmbH",
    "url": "https://hahnpro.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hahnprojects/flow.git"
  },
  "bin": {
    "flow": "lib/cli.mjs"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint '**/*.{js,mjs}'",
    "test": "jest"
  },
  "dependencies": {
    "archiver": "7.0.1",
    "axios": "1.12.0",
    "chalk": "5.6.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "class-validator-jsonschema": "5.1.0",
    "commander": "14.0.0",
    "copyfiles": "2.4.1",
    "dotenv": "17.2.2",
    "ejs": "3.1.10",
    "execa": "9.6.0",
    "express": "5.1.0",
    "form-data": "4.0.4",
    "get-port": "7.1.0",
    "glob": "11.0.3",
    "https-proxy-agent": "7.0.6",
    "nconf": "0.13.0",
    "open": "10.2.0",
    "openid-client": "5.7.1",
    "ora": "8.2.0",
    "reflect-metadata": "0.2.2",
    "ts-node": "10.9.2"
  },
  "devDependencies": {
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/nconf": "0.10.7",
    "@types/node": "22.18.1",
    "eslint": "8.57.1",
    "eslint-plugin-unicorn": "56.0.1",
    "jest": "30.1.3",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=v22"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:prettier/recommended",
      "plugin:unicorn/all"
    ],
    "rules": {
      "unicorn/no-array-reduce": "off",
      "unicorn/no-null": "off",
      "unicorn/prefer-string-replace-all": "off",
      "unicorn/prefer-object-from-entries": "off",
      "no-async-promise-executor": "off",
      "no-console": "error"
    }
  }
}
