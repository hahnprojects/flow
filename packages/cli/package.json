{
  "name": "@hahnpro/flow-cli",
  "version": "2.17.1",
  "description": "CLI for managing Flow Modules",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Hahn Projects GmbH",
    "url": "https://hahnpro.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hahnprojects/flow.git"
  },
  "bin": {
    "flow": "lib/cli.mjs"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint '**/*.{js,mjs}'",
    "test": "jest"
  },
  "dependencies": {
    "archiver": "7.0.1",
    "axios": "1.6.7",
    "chalk": "5.3.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "class-validator-jsonschema": "5.0.0",
    "commander": "12.0.0",
    "copyfiles": "2.4.1",
    "dotenv": "16.4.5",
    "ejs": "3.1.9",
    "execa": "8.0.1",
    "express": "4.19.2",
    "form-data": "4.0.0",
    "get-port": "7.0.0",
    "glob": "10.3.10",
    "https-proxy-agent": "7.0.4",
    "nconf": "0.12.1",
    "open": "10.1.0",
    "openid-client": "5.6.5",
    "ora": "8.0.1",
    "reflect-metadata": "0.2.1",
    "ts-node": "10.9.2"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/nconf": "0.10.6",
    "@types/node": "20.11.26",
    "eslint": "8.57.0",
    "eslint-plugin-unicorn": "51.0.1",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "typescript": "5.4.2"
  },
  "engines": {
    "node": ">=v18"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:prettier/recommended",
      "plugin:unicorn/all"
    ],
    "rules": {
      "unicorn/no-array-reduce": "off",
      "unicorn/no-null": "off",
      "unicorn/prefer-string-replace-all": "off",
      "unicorn/prefer-object-from-entries": "off",
      "no-async-promise-executor": "off",
      "no-console": "error"
    }
  }
}
