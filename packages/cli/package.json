{
  "name": "@hahnpro/flow-cli",
  "version": "2.11.0",
  "description": "CLI for managing Flow Modules",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Hahn Projects GmbH",
    "url": "https://hahnpro.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hahnprojects/flow.git"
  },
  "bin": {
    "flow": "lib/cli.mjs"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint '*/**/*.{js,mjs}'",
    "test": "jest"
  },
  "dependencies": {
    "archiver": "^5.3.0",
    "axios": "^0.24.0",
    "chalk": "^5.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "~0.13.2",
    "class-validator-jsonschema": "^3.1.0",
    "commander": "^8.3.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "execa": "^6.0.0",
    "form-data": "^4.0.0",
    "glob": "^7.2.0",
    "https-proxy-agent": "^5.0.0",
    "ora": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "eslint": "^8.3.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "jest": "^27.4.2",
    "prettier": "^2.5.0",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:prettier/recommended",
      "plugin:unicorn/all"
    ],
    "rules": {
      "unicorn/no-array-reduce": "off",
      "unicorn/no-null": "off",
      "unicorn/prefer-object-from-entries": "off",
      "no-async-promise-executor": "off",
      "no-console": "error"
    }
  }
}
