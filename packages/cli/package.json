{
  "name": "@hahnpro/flow-cli",
  "version": "2.16.3",
  "description": "CLI for managing Flow Modules",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Hahn Projects GmbH",
    "url": "https://hahnpro.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hahnprojects/flow.git"
  },
  "bin": {
    "flow": "lib/cli.mjs"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint '**/*.{js,mjs}'",
    "test": "jest"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "axios": "~1.3.6",
    "chalk": "^5.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "~0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "commander": "^10.0.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "execa": "^7.1.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "get-port": "^6.1.2",
    "glob": "^9.3.5",
    "https-proxy-agent": "^5.0.1",
    "nconf": "^0.12.0",
    "open": "^9.1.0",
    "openid-client": "^5.4.1",
    "ora": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/nconf": "^0.10.3",
    "@types/node": "^18.16.0",
    "eslint": "^8.39.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:prettier/recommended",
      "plugin:unicorn/all"
    ],
    "rules": {
      "unicorn/no-array-reduce": "off",
      "unicorn/no-null": "off",
      "unicorn/prefer-string-replace-all": "off",
      "unicorn/prefer-object-from-entries": "off",
      "no-async-promise-executor": "off",
      "no-console": "error"
    }
  }
}
