{
  "name": "@hahnpro/hpc-api",
  "version": "5.3.2",
  "description": "Module for easy access to the HahnPRO API",
  "license": "MIT",
  "author": {
    "name": "Hahn Projects GmbH",
    "url": "https://hahnpro.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hahnprojects/flow.git"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
