  "dependencies": {
    "axios": "1.7.9",
    "eventsource": "3.0.1",
    "form-data": "4.0.1",
    "jose": "5.9.6",
    "jwt-decode": "4.0.0",
    "p-queue": "6.6.2",
    "ts-mixer": "6.0.4",
    "uuid": "11.0.3"
  },
  "devDependencies": {
    "@types/eventsource": "1.1.15",
    "axios-mock-adapter": "2.1.0"
  },
  "engines": {
    "node": ">=v18"
  }
